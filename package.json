{
  "name": "veilarbvedtaksstottefs",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite --mode demo",
    "build": "tsc && vite build --base=https://cdn.nav.no/obo/prod/veilarbvedtaksstottefs/dist",
    "build:dev": "tsc && vite build --base=https://cdn.nav.no/obo/dev/veilarbvedtaksstottefs/dist",
    "build:mock": "tsc && vite build --mode demo --base=/veilarbvedtaksstottefs/",
    "lint": "eslint src --max-warnings 0",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prettier:check": "prettier --check src --config .prettierrc.json",
    "prettier:write": "prettier --write src --config .prettierrc.json",
    "preview": "vite preview",
    "prepare": "husky",
    "test": "vitest"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@navikt/aksel-icons": "7.32.2",
    "@navikt/arbeidssokerregisteret-utils": "1.8.3",
    "@navikt/ds-css": "7.32.2",
    "@navikt/ds-react": "7.32.2",
    "@navikt/fnrvalidator": "2.1.5",
    "@navikt/navspa": "7.1.0",
    "axios": "1.12.2",
    "classnames": "2.5.1",
    "constate": "3.3.3",
    "dayjs": "1.11.18",
    "dompurify": "3.3.0",
    "lodash.debounce": "4.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-pdf": "9.2.1"
  },
  "devDependencies": {
    "@eslint/js": "9.37.0",
    "@navikt/aksel-stylelint": "7.32.2",
    "@types/lodash.debounce": "4.0.9",
    "@types/react": "19.1.17",
    "@types/react-dom": "19.1.11",
    "@vitejs/plugin-react": "5.0.4",
    "eslint": "9.37.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.0",
    "eslint-plugin-react-refresh": "0.4.24",
    "globals": "16.4.0",
    "husky": "9.1.7",
    "less": "4.4.2",
    "lint-staged": "16.2.4",
    "msw": "2.11.5",
    "prettier": "3.6.2",
    "stylelint": "16.25.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.1",
    "vite": "7.1.11",
    "vitest": "3.2.4"
  },
  "lint-staged": {
    "src/**/*.css": [
      "stylelint"
    ],
    "src/**/*.{json,less,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  },
  "stylelint": {
    "extends": "@navikt/aksel-stylelint/recommended"
  }
}
