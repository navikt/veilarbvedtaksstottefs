{
  "name": "veilarbvedtaksstottefs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite --mode demo",
    "build": "tsc && vite build --base=https://cdn.nav.no/obo/prod/veilarbvedtaksstottefs/dist",
    "build:dev": "tsc && vite build --base=https://cdn.nav.no/obo/dev/veilarbvedtaksstottefs/dist",
    "build:mock": "tsc && vite build --mode demo --base=/veilarbvedtaksstottefs/",
    "lint": "eslint src --max-warnings 0",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prettier:check": "prettier --check src --config .prettierrc.json",
    "prettier:write": "prettier --write src --config .prettierrc.json",
    "preview": "vite preview",
    "prepare": "husky",
    "test": "vitest"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@navikt/aksel-icons": "7.25.1",
    "@navikt/arbeidssokerregisteret-utils": "1.7.0",
    "@navikt/ds-css": "7.25.1",
    "@navikt/ds-react": "7.25.1",
    "@navikt/fnrvalidator": "2.1.5",
    "@navikt/navspa": "5.0.1",
    "axios": "1.11.0",
    "classnames": "2.5.1",
    "constate": "3.3.3",
    "dayjs": "1.11.13",
    "dompurify": "3.2.6",
    "lodash.debounce": "4.0.8",
    "lodash.isequal": "4.5.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-pdf": "6.2.2"
  },
  "devDependencies": {
    "@navikt/aksel-stylelint": "7.25.1",
    "@types/jest": "29.5.14",
    "@types/lodash.debounce": "4.0.9",
    "@types/lodash.isequal": "4.5.8",
    "@types/react": "17.0.57",
    "@types/react-dom": "17.0.19",
    "@vitejs/plugin-react": "4.7.0",
    "eslint": "8.57.1",
    "eslint-plugin-react": "7.37.5",
    "husky": "9.1.7",
    "less": "4.4.0",
    "lint-staged": "15.4.3",
    "msw": "2.10.4",
    "prettier": "3.6.2",
    "stylelint": "16.22.0",
    "typescript": "4.9.5",
    "vite": "7.0.6",
    "vitest": "3.2.4"
  },
  "lint-staged": {
    "src/**/*.css": [
      "stylelint"
    ],
    "src/**/*.{json,less,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  },
  "stylelint": {
    "extends": "@navikt/aksel-stylelint/recommended"
  }
}
