{
  "name": "veilarbvedtaksstottefs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env REACT_APP_DEV=true craco start",
    "build": "cross-env craco build",
    "build:mock": "cross-env PUBLIC_URL=/veilarbvedtaksstottefs REACT_APP_DEV=true craco build",
    "test": "craco test",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "husky install",
    "pre-commit": "lint-staged --verbose"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{json,less,css,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --config .eslintrc.lintstaged.json"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@craco/craco": "7.1.0",
    "@navikt/aksel-icons": "5.18.3",
    "@navikt/ds-css": "5.18.3",
    "@navikt/ds-react": "5.18.3",
    "@navikt/fnrvalidator": "1.3.0",
    "@navikt/navspa": "5.0.1",
    "axios": "1.6.7",
    "classnames": "2.5.1",
    "constate": "3.3.2",
    "craco-less": "3.0.1",
    "dayjs": "1.11.10",
    "dompurify": "3.0.9",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "nav-frontend-core": "^6.0.1",
    "nav-frontend-tekstomrade": "3.0.2",
    "nav-frontend-typografi": "4.0.2",
    "nav-frontend-typografi-style": "2.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-pdf": "5.7.2",
    "react-scripts": "5.0.1",
    "typescript": "4.7.4"
  },
  "devDependencies": {
    "@types/classnames": "2.3.1",
    "@types/dompurify": "3.0.5",
    "@types/jest": "29.5.12",
    "@types/lodash.debounce": "4.0.9",
    "@types/lodash.isequal": "4.5.8",
    "@types/react": "17.0.57",
    "@types/react-dom": "17.0.19",
    "@types/react-pdf": "5.7.4",
    "cross-env": "7.0.3",
    "husky": "8.0.3",
    "lint-staged": "15.2.2",
    "msw": "2.2.1",
    "prettier": "3.2.5"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
